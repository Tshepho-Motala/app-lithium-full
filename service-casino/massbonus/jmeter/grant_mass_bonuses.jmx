              <stringProp name="parameters"></stringProp>
              <stringProp name="script">log.info(&quot; ------------  Slot API Bonus Trigger  ------------ &quot;);
import javax.crypto.Mac;
import javax.crypto.spec.SecretKeySpec;
import java.security.InvalidKeyException;
import groovy.json.JsonBuilder;
import groovy.json.JsonSlurper;
import groovy.json.JsonOutput;

log.info(vars.get(&quot;REQUEST_ID&quot;))
def requestId = Integer.parseInt(vars.get(&quot;REQUEST_ID&quot;));  // ((index*iteration_total)-iteration_total+iteration_count);
def bonus_code = vars.get(&quot;BONUS_CODE&quot;);
//def numberOfBets = Integer.parseInt(vars.get(&apos;numberOfBets&apos;));
//def amount = org.apache.commons.lang3.RandomUtils.nextInt(100, 500);
def amount = &quot;5.66&quot;;

log.info(&quot;Bonus Trigger (&quot;+requestId+&quot;)(&quot;+amount+&quot;) - bonus code : FIVES&quot;); //+numberOfBets);


def timestamp = System.currentTimeMillis();

vars.putObject(&quot;timestamp&quot;, timestamp);

log.info(&quot;+++++++++++++&quot;+vars.get(&quot;userGuid&quot;));
 String playerguid = vars.get(&quot;userGuid&quot;);
//String playerguid = &quot;livescore_uk/13850&quot;;
//log.info(playerguid+&quot; :: &quot;+vars.get(&quot;access_token&quot;));

String secretKey = vars.get(&quot;SLOTAPI_HASH_PASSWORD&quot;);
String data   = requestId + &quot;:&quot; + 
    amount;
    
Mac mac = Mac.getInstance(&quot;HmacSHA256&quot;);
SecretKeySpec secretKeySpec = new SecretKeySpec(secretKey.getBytes(), &quot;HmacSHA256&quot;);
mac.init(secretKeySpec);
byte[] digest = mac.doFinal(data.getBytes());
//encodedData = digest.toString();
StringBuffer sb = new StringBuffer();
for (byte b : digest) {
	sb.append(String.format(&quot;%02x&quot;, b &amp; 0xff));
}
encodedData = sb.toString().toLowerCase();
log.info(&quot;HMAC SHA256 base64: &quot; + encodedData);

def body = [
    &quot;playerGuid&quot;: playerguid,
    &quot;requestId&quot;: requestId,
    &quot;bonusCode&quot;: bonus_code,
    &quot;timestamp&quot;: timestamp,
    &quot;customAmountDecimal&quot;: amount,
    &quot;description&quot;: &quot;Jmeter Update&quot;,
    &quot;sha256&quot;: encodedData
];
def req_body = JsonOutput.toJson(body);

log.info(req_body);

vars.putObject(&apos;req_body&apos;, req_body);
</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
